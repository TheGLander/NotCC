{
	"compilerOptions": {
		"esModuleInterop": true,
		"target": "ES2016",
		"module": "CommonJS",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"outDir": "dist",
		"allowJs": true
	},
	"buildOptions": {
		"incremental": true
	},
	"include": ["./src/*"]
}
