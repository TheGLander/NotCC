{
	"applicationId": "notcc-player.glander.club",
	"version": "1.0.0",
	"defaultMode": "window",
	"documentRoot": "/webDist/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": ["app.*", "os.*", "filesystem.*", "debug.log"],
	"modes": {
