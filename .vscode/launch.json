{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach to browser (Chrome)",
			"port": 9222,
			"request": "attach",
			"type": "chrome",
			"pathMapping": {
				"gamePlayer/src": "${workspaceFolder}/gamePlayer/src/",
				"logic": "${workspaceFolder}/logic/src/"
			},
			"sourceMaps": true,
			"urlFilter": "http://localhost:5173/*"
		}
	]
}
