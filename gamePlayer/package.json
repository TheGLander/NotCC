{
	"name": "@notcc/player",
	"private": true,
	"version": "2.0.0",
	"scripts": {
		"dev": "vite",
		"build:ssg": "SSG=true vite build -c vite.config.js --ssr src/main-ssg.tsx --outDir dist/ssg",
		"build": "rm -rf dist && tsc && pnpm build:ssg && vite build && rm -r dist/ssg",
		"build:neu": "VITE_BUILD_PLATFORM=desktop pnpm build && zx makeDists.mjs",
		"build:neu-zip": "pnpm build:neu && rm -f NotCC.zip && cd artifacts && zip -r ../NotCC.zip .",
		"build:full": "pnpm build:neu-zip && rm -r dist && pnpm build && zx makeWebDownloads.mjs",
		"dev:neu": "neu run",
		"prepublish": "neu update"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheGLander/NotCC.git"
	},
	"author": "G lander",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/TheGLander/NotCC/issues"
	},
	"homepage": "https://github.com/TheGLander/NotCC#readme",
	"type": "module",
	"devDependencies": {
		"@neutralinojs/lib": "^5.3.0",
		"@neutralinojs/neu": "^11.2.2",
		"@notcc/logic": "workspace:^",
		"@preact/preset-vite": "^2.6.0",
		"@types/clone": "^2.1.4",
		"@types/is-hotkey": "^0.1.9",
		"@types/node": "^15.14.9",
		"@types/path-browserify": "^1.0.2",
		"autoprefixer": "^10.4.16",
		"clone": "^2.1.2",
		"fast-printf": "^1.6.9",
		"fflate": "^0.7.4",
		"idb-keyval": "^6.2.1",
		"is-hotkey": "^0.2.0",
		"jotai": "^2.10.1",
		"jotai-effect": "^1.0.3",
		"lz-string": "^1.5.0",
		"path-browserify": "^1.0.1",
		"postcss": "^8.4.31",
		"preact": "^10.19.2",
		"preact-render-to-string": "^6.3.1",
		"react": "npm:@preact/compat",
		"react-dom": "npm:@preact/compat",
		"react-draggable": "^4.4.6",
		"react-responsive": "^9.0.2",
		"suspend-react": "^0.1.3",
		"tailwind-merge": "^2.0.0",
		"tailwindcss": "^3.3.5",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vite-plugin-pwa": "^1.0.2",
		"workbox-window": "^7.3.0"
	},
	"dependencies": {
		"@dagrejs/dagre": "^1.0.4",
		"react-error-boundary": "^4.0.13"
	}
}
